{
  "dashboard": {
    "id": null,
    "title": "FastAPI Monitoring",
    "tags": ["fastapi", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(request_count_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Request Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(request_latency_seconds_sum[5m]) / rate(request_latency_seconds_count[5m])",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Logs",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"fastapi-app\"}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 8}
      }
    ],
    "time": {"from": "now-6h", "to": "now"},
    "timepicker": {},
    "templating": {"list": []},
    "refresh": "5s",
    "schemaVersion": 16,
    "version": 0,
    "uid": "fastapi-monitoring"
  }
}